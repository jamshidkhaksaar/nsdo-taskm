{
  "name": "nsdo-taskm",
  "version": "1.0.0",
  "private": true,
  "description": "Task Management System with NestJS and React",
  "scripts": {
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend-nest && npm run start:dev",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend-nest && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend-nest && npm install",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "dependencies": {
    "axios": "^1.8.1",
    "bcrypt": "^5.1.1",
    "dayjs": "^1.11.13",
    "mysql2": "^3.12.0",
    "nest-commander": "^3.16.0",
    "recharts": "^2.15.0",
    "typeorm": "^0.3.21",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
