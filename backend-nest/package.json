{
  "name": "backend-nest",
  "version": "0.0.1",
  "description": "NSDO Task Management System",
  "author": "Jamshid Khaksaar",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "create:admin": "ts-node -r tsconfig-paths/register src/cli.ts create-admin",
    "typeorm": "typeorm-ts-node-commonjs -d ./src/data-source.ts",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "create:admin:simple": "ts-node -r tsconfig-paths/register src/scripts/create-admin-simple.ts",
    "migrate:user-roles": "ts-node -r tsconfig-paths/register src/scripts/migrate-user-roles.ts",
    "migrate:user-roles:manual": "ts-node -r tsconfig-paths/register src/scripts/manual-role-migration.ts",
    "check:env": "ts-node -r tsconfig-paths/register src/scripts/check-env.ts",
    "prepare:db": "npm run check:env && npm run migration:run && npm run migrate:user-roles",
    "migration:run:prod": "typeorm-ts-node-commonjs migration:run -d dist/data-source.js"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.6.1",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.0",
    "@nestjs/swagger": "^11.0.6",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.0",
    "@sendgrid/mail": "^8.1.5",
    "@types/ioredis": "^4.28.10",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.49.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csurf": "^1.2.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "mysql2": "^3.12.0",
    "nest-commander": "^3.16.1",
    "nodemailer": "^6.10.1",
    "nsdo-taskm": "file:..",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.13.3",
    "pg-promise": "^11.13.0",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "uuid": "^11.1.0",
    "@nestjs/cli": "^11.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/csurf": "^1.11.5",
    "@types/d3": "^7.4.3",
    "@types/d3-axis": "^3.0.6",
    "@types/d3-brush": "^3.0.6",
    "@types/d3-contour": "^3.0.6",
    "@types/d3-dsv": "^3.0.7",
    "@types/d3-quadtree": "^3.0.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^6.4.17",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.24",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
